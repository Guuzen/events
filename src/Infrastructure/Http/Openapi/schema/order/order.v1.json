{
  "title": "order",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "eventId": {
      "type": "string",
      "format": "uuid"
    },
    "tariffId": {
      "type": "string",
      "format": "uuid"
    },
    "paid": {
      "type": "boolean"
    },
    "price": {
      "$ref": "../money.v1.json"
    },
    "cancelled": {
      "type": "boolean"
    },
    "userId": {
      "type": "string",
      "format": "uuid"
    },
    "makedAt": {
      "type": "string",
      "format": "date-time"
    },
    "promocode": {
      "type": "object",
      "nullable": true,
      "properties": {
        "discount": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "amount": {
              "anyOf": [
                {
                  "$ref": "../money.v1.json"
                }
              ]
            }
          },
          "required": [
            "type",
            "amount"
          ]
        },
        "id": {
          "type": "string",
          "format": "uuid"
        }
      }
    },
    "productType": {
      "type": "string",
      "enum": [
        "ticket"
      ]
    }
  },
  "required": [
    "id",
    "eventId",
    "tariffId",
    "paid",
    "price",
    "cancelled",
    "userId",
    "makedAt"
  ]
}